{
	"Table of Contents": {
		"prefix": ["table", "toc"],
		"description": "A table of contents for the current page or chapter.",
		"body": ["{%", "${1:toc} = toc(\"${0:Content}\")", "%}", "{{${1:toc}}}"]
	},
	"Gyro Embedding": {
		"prefix": ["gy", "gyro"],
		"description": "Embed Gyro code.",
		"body": ["{%", "$0", "%}"]
	},
	"Variable Embedding": {
		"prefix": ["var"],
		"description": "Embed a variable.",
		"body": ["{{", "${0:name}", "}}"]
	},
	"Insert Table": {
		"prefix": ["table"],
		"description": "Insert a table.",
		"body": ["|", "${0:Column 1}", "|", "${1:Column 2}", "|", "\n| ------- | ------- |\n|", "${2:Value 1}", "|", "${3:Value 2}", "|"]
	},
	"Auto resolving Link": {
		"prefix": ["!"],
		"description": "Auto resolving link.",
		"body": ["!`${0:name}`"]
	}
}